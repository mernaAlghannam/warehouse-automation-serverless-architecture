{
  "openapi": "3.0.3",
  "info": {
    "title": "Warehouse Automation",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://shipping-data-api.azurewebsites.net/api/"
    }
  ],
  "tags": [
    {
      "name": "shipping information",
      "description": "Everything about shipping informtion"
    }
  ],
  "paths": {
    "/update-shipping-data": {
      "post": {
        "tags": [
          "shipping information"
        ],
        "summary": "Add a new pet to the store",
        "description": "Add a new pet to the store",
        "operationId": "post-update-shipping-data",
        "parameters": [
          {
            "name": "code",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Create a new pet in the store",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Shipper"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Shipper"
                }
              }
            }
          },
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/get-shipping-data": {
      "get": {
        "tags": [
          "shipping information"
        ],
        "summary": "Finds Pets by status",
        "description": "Multiple status values can be provided with comma separated strings",
        "operationId": "get-shipping-data",
        "parameters": [
          {
            "name": "code",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Shipper"
                  }
                }
              },
              "application/xml": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Shipper"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid status value"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Shipper": {
        "required": [
          "name",
          "photoUrls"
        ],
        "type": "object",
        "properties": {
          "received": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "Date": {
                  "type": "string",
                  "example": "Mar 11, 2022"
                },
                "WarehouseID": {
                  "type": "string",
                  "example": "a908cef7-4c67-40f3-88f7-08a03ba4104e"
                },
                "ShippingPO": {
                  "type": "string",
                  "example": "3f7b2654-052d-4a4e-905f-87f22a3877a9"
                },
                "ShipmentID": {
                  "type": "string",
                  "example": 4
                },
                "BoxesRcvd": {
                  "type": "string",
                  "example": 4
                }
              }
            }
          }
        }
      }
    }
  }
}